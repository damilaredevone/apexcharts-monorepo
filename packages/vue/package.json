{
  "name": "@damilaredev/apexcharts-vue",
  "version": "0.0.0",
  "description": "Vue 3 ApexCharts",
  "type": "module",
  "packageManager": "pnpm@7.26.3",
  "engines": {
    "node": "^14.16.0 || ^16.10.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "vue-tsc --noEmit && tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@damilaredev/utils": "^0.5.0"
  },
  "devDependencies": {
    "vue-tsc": "^1.0.24",
    "typescript": "4.9.5"
  },
  "peerDependencies": {
    "apexcharts": "^3",
    "vue": "^3"
  },
  "peerDependenciesMeta": {
    "apexcharts": {
      "optional": false
    }
  }
}
